{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest --run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc",
    "compile": "tsc --noEmit --watch",
    "test:e2e": "cypress open --e2e",
    "test:e2e:headless": "cypress run --e2e --browser chrome",
    "test:integration": "cypress open -C cypress.integration.ts",
    "test:integration:headless": "cypress run -C cypress.integration.ts --browser chrome"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^1.5.0",
    "buffer": "^6.0.3",
    "firebase": "^9.14.0",
    "google-libphonenumber": "^3.2.33",
    "i18next": "^23.4.2",
    "i18next-http-backend": "^2.2.1",
    "js-cookie": "^3.0.5",
    "node-forge": "^1.3.1",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.3",
    "react-icons": "^4.12.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.7.4",
    "vite-plugin-mkcert": "^1.17.2"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@faker-js/faker": "^8.4.1",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/js-cookie": "^3.0.6",
    "@types/node-forge": "^1.3.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^13.13.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-intercept-formdata": "^0.6.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jsdom": "^21.1.0",
    "postcss": "^8.4.27",
    "prettier": "^2.7.1",
    "prettier-config-nick": "^1.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.0.0",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-no-target-blank": "off"
    }
  },
  "prettier": "prettier-config-nick",
  "packageManager": "yarn@4.3.1"
}
